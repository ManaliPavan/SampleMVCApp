
@{
    ViewBag.Title = "ProductList";
}

<h2>ProductList</h2>
<div>
    <ol>
        <!-- 'at-d-ret' sign is called razor syntax in mvc it is use to write C# code in the html page-->
        @foreach (var item in ViewBag.Products)
        {
            <li>
                @item
            </li>
        }
    </ol>
</div>
